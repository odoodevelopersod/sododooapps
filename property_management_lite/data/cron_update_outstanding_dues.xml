<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Update Outstanding Dues Daily -->
    <record id="cron_update_outstanding_dues" model="ir.cron">
        <field name="name">Update Outstanding Dues</field>
        <field name="model_id" ref="model_property_outstanding_dues"/>
        <field name="state">code</field>
        <field name="code">model.update_outstanding_dues()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active">True</field>
    </record>
</odoo>
