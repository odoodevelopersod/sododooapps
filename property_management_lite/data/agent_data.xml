            <field name="phone">+971 50 123 4567</field>
            <field name="email">ahmed.almansouri@shobharealestate.com</field>
            <field name="category_id" eval="[(6, 0, [ref('partner_category_property_agent')])]"/>
        </record>
        
        <record id="sample_agent_2" model="res.partner">
            <field name="name">Sarah Johnson</field>
