<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Partner Categories for Agents -->
        <record id="partner_category_property_agent" model="res.partner.category">
            <field name="name">Property Agent</field>
            <field name="color">3</field>
        </record>
        
        <record id="partner_category_rental_agent" model="res.partner.category">
            <field name="name">Rental Agent</field>
            <field name="color">5</field>
        </record>
        
        <record id="partner_category_sales_agent" model="res.partner.category">
            <field name="name">Sales Agent</field>
            <field name="color">7</field>
        </record>
        
        <!-- Sample Agent Records -->
        <record id="sample_agent_1" model="res.partner">
            <field name="name">Ahmed Al-Mansouri</field>
            <field name="is_company">False</field>
