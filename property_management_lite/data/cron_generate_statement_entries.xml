<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Scheduled Action: Generate Missing Statement Entries -->
    <record id="ir_cron_generate_statement_entries" model="ir.cron">
        <field name="name">Generate Missing Statement Entries</field>
        <field name="model_id" ref="model_property_agreement"/>
        <field name="state">code</field>
        <field name="code">model.cron_generate_missing_statement_entries()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="False"/>
    </record>
</odoo>
