<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Scheduled Action: Recompute Outstanding Dues -->
    <record id="ir_cron_recompute_outstanding_dues" model="ir.cron">
        <field name="name">Recompute Outstanding Dues</field>
        <field name="model_id" ref="model_property_agreement"/>
        <field name="state">code</field>
        <field name="code">model.cron_recompute_outstanding_dues()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="False"/>
    </record>
</odoo>
